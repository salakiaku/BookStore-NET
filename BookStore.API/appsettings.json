{
  "ConnectionStrings": {
    "Default": "Data Source=(LocalDB)\\MSSQLLocalDB;Initial Catalog=BookStoreDb;Integrated Security=True;Pooling=False;Encrypt=True;Trust Server Certificate=False",
    "ITLSConnectionString": "Data Source=(localdb)\\MSSQLLocalDB; db=base_desenv;Integrated Security=True;Pooling=False;Encrypt=True;Trust Server Certificate=False"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.LifeTime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]

  },
  "AllowedHosts": "*"
}
